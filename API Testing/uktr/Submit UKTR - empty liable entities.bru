meta {
  name: Submit UKTR - empty liable entities
  type: http
  seq: 9
}

post {
  url: {{baseUrl}}/RESTAdapter/PLR/UKTaxReturn
  body: json
  auth: none
}

headers {
  Content-Type: application/json
  Authorization: {{validBearerToken}}
  X-Pillar2-Id: {{domesticOnlyPlrId}}
}

body:json {
  {
    "accountingPeriodFrom": "2024-01-01",
    "accountingPeriodTo": "2024-03-31",
    "obligationMTT": false,
    "electionUKGAAP": false,
    "liabilities": {
      "electionDTTSingleMember": false,
      "electionUTPRSingleMember": false,
      "numberSubGroupDTT": 4,
      "numberSubGroupUTPR": 5,
      "totalLiability": 5000.99,
      "totalLiabilityDTT": 5000.99,
      "totalLiabilityIIR": 0,
      "totalLiabilityUTPR": 0,
      "liableEntities": []
    }
  }
}

assert {
  res.status: eq 422
  res.body.errors.code: eq "093"
  res.body.errors.text: eq "liabilityEntity cannot be empty"
}
